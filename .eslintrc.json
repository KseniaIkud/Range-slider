{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true,
        "jest": true,
        "jquery": true
    },
    "extends": [
        "airbnb-typescript/base",
        "plugin:fsd/all"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module",
        "project": "./tsconfig.json"
    },
    "plugins": [
        "@typescript-eslint",
        "fsd"
    ],
    "rules": {
        "class-methods-use-this": "off",
        "no-param-reassign": "off"
    }
}
